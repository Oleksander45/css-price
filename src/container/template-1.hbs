
    <div class="page p1">
        <div class="page-content">
            <div class="page-block">
                <header class="header row g-0">
                    {{>logo}}
                    <nav class="header-nav col-9">
                        <div class="head-nav-right">
                            {{>nav list=header }}
                        </div>
                    </nav>
                </header>
                {{>divider}}
            </div>
             <div class="page-block">
              {{>hero data=hero}}
            </div>
            <div class="page-block">
              <main class="row g-0 plans">                
               {{>plans}}                
              </main>
            </div>
            <div class="page-block">
                {{>divider}}
                <footer class="row ">
                    <div class="col-3">
                         {{>logo}}
                         <span class="copyright">Copyright 2023-2024</span>
                    </div>
                    {{#each footer}}
                    <div class="col-3">
                       
                        <span class="foot-title">{{title}}</span>
                        {{>nav list=links isVertical=true}}
                    </div>
                    {{/each}}
                </footer>
            </div>
        </div>
    </div>
